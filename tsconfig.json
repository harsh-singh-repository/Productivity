{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",  // Change this to a valid value
    "target": "es5",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
